<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog"/>
  </configSections>
  <appSettings>
    <!-- It should be possible to add more folder keys to determine where to find types of Media -->
    <!--<add key="comicFolder" value="C:\Development (Thuis)\WWW.DE-BAAY.NL Website Development\WWW.DE-BAAY.NL Online Comic Reader\WWW.DE-BAAY.NL Online Comic Reader\Comics"/>-->
    <!--add key="comicFolder" value="Comics/"/-->
    <add key="comicFolder" value="\\DE-BAAY.NL\DistributedFileShare\Comics\pub\"/>
    <!--add key="comicFolder" value="\\DE-BAAY.NL\AdministrativeDistributedShare\Archive\Comics\Overig\" /-->
    <add key="mediaFolder" value="Media/"/>
    <add key="imageFolder" value="Images/"/>
    <add key="musicFolder" value="Music/"/>
    <add key="fileFolder" value="Files/"/>
    <add key="epubFolder" value="Epubs/"/>
    <add key="pageLimit" value="10"/>
    <add key="defaultFile" value="Images/NotFound.png"/>
  </appSettings>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <targets>
      <target name="logfile" xsi:type="File" fileName="${aspnet-appbasepath}/Logs/APILogger.log" layout="${longdate}|${level:uppercase=true}|${logger}|${threadid}|${message}|${exception:format=tostring}"/>
    </targets>
    <rules>
      <logger name="*" minlevel="Info" writeTo="logfile"/>
    </rules>
  </nlog>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.7.1" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.7.2"/>
    <pages controlRenderingCompatibilityVersion="4.0"/>
  </system.web>
  <system.webServer>
    <rewrite>
      <rules>
        <rule name="RemoveHandler">
          <match url="(.*)"/>
          <action type="Rewrite" url="fetcher.ashx" appendQueryString="true"/>
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
