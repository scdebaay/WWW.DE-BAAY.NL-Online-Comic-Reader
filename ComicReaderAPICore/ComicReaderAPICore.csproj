  <ItemGroup>
    <None Remove="Images\Arctic Circle 2015-01-01.gif" />
  </ItemGroup>

  <ItemGroup>
    <None Include="C:\Users\scdebaay.DEBAAY\.nuget\packages\nlog.schema\4.7.0\contentFiles\any\any\NLog.xsd" Link="NLog.xsd">
